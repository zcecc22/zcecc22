auto lo
iface lo inet loopback

iface wlp3s0 inet dhcp
wpa-ssid ssid
wpa-psk password

auto enp1s0f0
iface enp1s0f0 inet static
address 172.30.42.1
netmask 255.255.255.0

auto enp1s0f1
iface enp1s0f1 inet static
address 192.168.178.2
netmask 255.255.255.0
gateway 192.168.178.1
up sysctl net.ipv6.conf.enp1s0f1.accept_ra=2
up systemctl restart wide-dhcpv6-client
